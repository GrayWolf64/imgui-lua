{
    "diagnostics.disable": [
        "undefined-global",
        "exp-in-action",
        "need-check-nil",
        "undefined-field",
        "inject-field",
        "lowercase-global",
        "unused-function",
        "unused-local"
    ],
    "checkThirdParty": false,
    "runtime.nonstandardSymbol": [
        "continue"
    ],
    "runtime.special": {
        "include": "dofile",
        "IncludeCS": "dofile"
    },
    "workspace.library": [
        "${addons}/garrysmod/module/library"
    ],
    "runtime.version": "LuaJIT",
    "runtime.plugin": "misc/luals_plugin.lua"
}